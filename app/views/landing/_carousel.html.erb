<div id=<%= pair %> class="carousel slide same-size bunch-slides" data-ride="carousel">

  <!-- Wrapper for slides -->
  <div class="carousel-inner same-size" role="listbox">
    <div class="item active jumbo-pic bunch-pic same-size <%= "top-#{pair}" %>">
    </div>

    <% unless pair == "mm" || pair == "mm-xs" %>
      <div class="item jumbo-pic bunch-pic same-size" data-src="<%= "middle-#{pair}" %>">
      </div>

      <div class="item jumbo-pic bunch-pic same-size" data-src="<%= "bottom-#{pair}" %>">
      </div>
    <% end %>
  </div>

  <!-- Controls -->
  <!-- <a class="left carousel&#45;control" href=<%#= pair %> role="button" data&#45;slide="prev"> -->
  <!--   <span class="sr&#45;only">Previous</span> -->
  <!-- </a> -->
  <!-- <a class="right carousel&#45;control" href=<%#= pair %> role="button" data&#45;slide="next"> -->
  <!--   <span class="sr&#45;only">Next</span> -->
  <!-- </a> -->
</div>
