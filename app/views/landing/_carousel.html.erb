<div id=<%= pair.delete('#') %> class="carousel slide" data-ride="carousel">

  <div class="panel panel-default card col-xs-12 sharp">
    <div class="panel-heading sharp">
      <div class="panel-title main-text">
        <h2><%= pair.delete('#').capitalize %></h2>
      </div>
    </div>
  </div>

  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target=<%= pair %> data-slide-to="0" class="active"></li>
    <li data-target=<%= pair %> data-slide-to="1"></li>
    <!-- <li data&#45;target=<%#= pair %> data&#45;slide&#45;to="2"></li> -->
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <%= one %>
      <div class="carousel-caption">
      </div>
    </div>

    <div class="item">
      <%= two %>
      <div class="carousel-caption">
      </div>
    </div>
    <!--  -->
    <!-- <div class="item"> -->
    <!--   <%#= three %> -->
    <!--   <div class="carousel&#45;caption"> -->
    <!--   </div> -->
    <!-- </div> -->
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href=<%= pair %> role="button" data-slide="prev">
    <span class="glyphicon glyphicon-circle-arrow-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href=<%= pair %> role="button" data-slide="next">
    <span class="glyphicon glyphicon-circle-arrow-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
