<div id=<%= pair.delete('#') %> class="carousel slide" data-ride="carousel">

  <div class="row bp-container">
    <div class="card sharp bp col-xs-12">
      <div class="main-text text-center">
        <h2><%= pair.delete('#').capitalize %></h2>
      </div>
    </div>
  </div>

  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target=<%= pair %> data-slide-to="0" class="active"></li>
    <li data-target=<%= pair %> data-slide-to="1"></li>
    <!-- <li data&#45;target=<%#= pair %> data&#45;slide&#45;to="2"></li> -->
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <%= one %>
    </div>

    <div class="item">
      <%= two %>
    </div>
    <!--  -->
    <!-- <div class="item"> -->
    <!--   <%#= three %> -->
    <!-- </div> -->
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href=<%= pair %> role="button" data-slide="prev">
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href=<%= pair %> role="button" data-slide="next">
    <span class="sr-only">Next</span>
  </a>
</div>
